{
  "name": "demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://davidcodina.github.io/React-useContext-useReducer-combineReducers-pattern/",
  "dependencies": {
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "bootstrap": "^5.1.0",
    "github-pages": "^3.0.2",
    "node-sass": "npm:sass@^1.32.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-compare-neg-zero": "warn",
      "no-cond-assign": "warn",
      "no-console": "off",
      "no-constant-condition": "warn",
      "no-dupe-else-if": "warn",
      "no-empty": "warn",
      "no-extra-boolean-cast": "warn",
      "no-extra-parens": "off",
      "no-extra-semi": "warn",
      "no-import-assign": "warn",
      "no-inner-declarations": [
        "off",
        "functions"
      ],
      "consistent-return": "off",
      "curly": [
        "warn",
        "all"
      ],
      "default-case": [
        "warn",
        {
          "commentPattern": "^no default$"
        }
      ],
      "guard-for-in": "warn",
      "no-alert": "off",
      "no-caller": "warn",
      "no-constructor-return": "warn",
      "no-else-return": [
        "warn",
        {
          "allowElseIf": true
        }
      ],
      "no-empty-function": "warn",
      "no-eq-null": "warn",
      "no-floating-decimal": "warn",
      "no-implicit-coercion": "warn",
      "no-return-assign": "off",
      "no-return-await": "warn",
      "yoda": "warn",
      "no-undefined": "warn",
      "block-spacing": [
        "warn",
        "always"
      ],
      "brace-style": [
        "off",
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "camelcase": [
        "warn",
        {
          "properties": "never",
          "ignoreDestructuring": false
        }
      ],
      "comma-spacing": [
        "warn",
        {
          "before": false,
          "after": true
        }
      ],
      "func-call-spacing": [
        "warn",
        "never"
      ],
      "implicit-arrow-linebreak": [
        "error",
        "beside"
      ],
      "no-lonely-if": "warn",
      "no-multi-assign": "warn",
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "one-var-declaration-per-line": [
        "warn",
        "always"
      ],
      "operator-assignment": [
        "warn",
        "always"
      ],
      "semi-spacing": [
        "warn",
        {
          "before": false,
          "after": true
        }
      ],
      "semi-style": [
        "warn",
        "last"
      ],
      "no-duplicate-imports": "warn",
      "no-var": "off",
      "prefer-const": [
        "warn",
        {
          "destructuring": "any",
          "ignoreReadBeforeAssign": true
        }
      ],
      "prefer-rest-params": "warn",
      "prefer-spread": "warn",
      "prefer-template": "warn",
      "template-curly-spacing": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
